<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{page_title}} | {{app_name}}</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css" integrity="sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ" crossorigin="anonymous">
  <!-- <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css"> -->
  <link href="https://unpkg.com/leaflet@1.0.1/dist/leaflet.css" media="all" rel="stylesheet">
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
  {{#metadata}}
    <link rel="canonical" href="{{url}}">
    <meta name="DC.title" content="{{title}}">
    <meta property="og:title" content="{{title}}">
    {{#creators}}
      <meta name="DC.creator" content="{{fullname}}">
    {{/creators}}
    <meta name="DC.description" content="{{description}}">
    <meta name="twitter:description" content="{{description}}">
    <meta property="og:description" content="{{description}}">
    <meta name="DC.type" content="{{type}}">
    <meta name="DC.created" content="{{created}}">
    <meta name="DC.modified" content="{{modified}}">
    <meta name="DC.date" content="{{modified}}">
    <meta property="og:locale" content="{{locale}}">
    <meta property="og:url" content="{{url}}">
    {{#image}}
      <meta property="og:image" content="{{url}}">
      <meta property="og:image:type" content="{{type}}">
      <meta property="og:image:height" content="{{height}}">
      <meta property="og:image:width" content="{{width}}">
      <meta name="twitter:image" content="{{url}}">
    {{/image}}
  {{/metadata}}
  <meta property="og:type" content="website" />
  <meta name="twitter:card" content="summary">
  <meta name="twitter:creator" content="@muziejus">
  <meta name="DC.contributor" content="SÃ¡ Pereira, Moacir P. de">
    <meta name="twitter:image" content="http://nywalker.newyorkscapes.org/images/apple-touch-icon.png" />
  <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png">
  <link rel="manifest" href="/site.webmanifest">
  <link rel="mask-icon" href="/images/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#ffffff">
  {{{ stylesheets }}}
  <script src="https://unpkg.com/leaflet@1.0.1/dist/leaflet.js"></script>
  <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js" integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn" crossorigin="anonymous"></script>
