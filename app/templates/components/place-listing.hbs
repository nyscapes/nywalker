<div class="place">
  {{#link-to "places.show" place.slug}}{{place.name}}{{/link-to}}
  <!-- {{place.name}} -->
  <small>
    {{#if place.flagged}}
      {{fa-icon "flag"}}
    {{/if}}
    {{#if place.note}}
      {{fa-icon "comment"}}
    {{/if}}
    ({{place.instance_count}} instances)
  </small>
</div>
